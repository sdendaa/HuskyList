<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tcss450.uw.edu.mynewapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconimage"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CategoryActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Authenticate.SignInActivity" />
        <activity
            android:name=".BookActivity"
            android:label="@string/title_activity_book"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Authenticate.RegisterActivity"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddItemActivity"
            android:label="@string/title_activity_add_book"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ComputerActivity"
            android:label="@string/title_activity_computer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CellPhoneActivity"
            android:label="@string/title_activity_cell_phone"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VideoGameActivity"
            android:label="@string/title_activity_video_game"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HouseHoldActivity"
            android:label="@string/title_activity_house_hold"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VehiclesActivity"
            android:label="@string/title_activity_vehicles"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UpdateAddsActivity"
            android:label="@string/title_activity_update_adds"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>